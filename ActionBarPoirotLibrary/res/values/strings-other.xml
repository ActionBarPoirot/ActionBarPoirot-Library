    <!-- from AOSP -->
    <!-- Title for a button to expand the list of activities in ActivityChooserView [CHAR LIMIT=25] -->
    <string name="activity_chooser_view_see_all">See all</string>
    <!-- Title default for a dialog showing possible activities in ActivityChooserView [CHAR LIMIT=25]. Changed for Collectionista! -->
    <string name="activity_chooser_view_dialog_title_default">Choose action</string>
