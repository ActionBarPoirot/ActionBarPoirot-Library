    <string name="shareactionprovider_share_with_application">Share with <xliff:g id="application_name" example="Bluetooth">%s</xliff:g></string>
    
    
    <!-- Tasks -->
    <string name="task_no_room">There are currently too many background tasks. Please try again in a moment.</string>
    <string name="progress_failed_product_exists">This product already exists in your collection.</string>
    <string name="progress_failed_task_exists">Another similar task is already pending or running.</string>
    
